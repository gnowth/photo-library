    "@types/node": "18.14.1",
    "@types/react": "18.0.28",
    "eslint-config-next": "13.2.1",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "prettier": "2.8.4"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
